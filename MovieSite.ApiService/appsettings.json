{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

    "Aspire": {
      "Npgsql": {
        "EntityFrameworkCore": {
          "PostgreSQL": {
            "ConnectionString": "YOUR_CONNECTIONSTRING",
            "DbContextPooling": true,
            "DisableHealthChecks": true,
            "DisableTracing": true
          }
        }
      }
    },
  "AllowedHosts": "*"
}
